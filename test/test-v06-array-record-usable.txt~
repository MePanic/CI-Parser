amodule v06;

var

v : array[10] of
  array[20] of integer;

c : record
  a, b : integer;
  c : array[10] of integer;
  d : record
    x, y, z : integer;
    u, v : record
      w : array[5] of integer;
      t : integer;
      end;
    end;
  end;	 	 

begin
 v[2][7] := 99;
 c.c[2]  := v[2][7];
 c.b     := c.c[2];
 c.d.y   := c.b;
 c.d.u.t := c.d.y;
 c.d.v.w[2] := c.d.u.t;
 
 v[1][1] := c.d.v.w[2];
 print v[1][1];
end v06. 
